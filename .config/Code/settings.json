{
  "editor.tabSize": 2,
  "workbench.startupEditor": "none",
  "explorer.confirmDelete": false,
  "editor.fontLigatures": false,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.renderLineHighlight": "gutter",
  "editor.hideCursorInOverviewRuler": true,
  "editor.lightbulb.enabled": false,
  "editor.cursorStyle": "line",
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?_-",
  "editor.wordBasedSuggestions": false,
  "editor.accessibilitySupport": "off",
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "off"
  },
  "files.exclude": {
    "**/*.bs.js": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.next": true,
    "**/__pycache__/**": true,
    "**/node_modules/": true,
    "**/npm-debug.log": true,
    "**/package-lock.json": true,
    "**/yarn.lock": true,
    ".mypy_cache/**": true,
    "vendor": true
  },
  "workbench.panel.defaultLocation": "bottom",
  "workbench.sideBar.location": "right",
  "search.smartCase": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "dist": true,
    "out": true,
    "coverage": true
  },
  "files.trimTrailingWhitespace": true,
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.leader": "<space>",
  "vim.useSystemClipboard": false,
  "vim.useCtrlKeys": true,
  "vim.foldfix": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(255, 127, 0, 0.6)",
  "vim.highlightedyank.textColor": "black",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-j>": false,
    "<C-,>": false
  },
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["leader", "f", "s"],
      "commands": [":w!"]
    },
    {
      "before": ["leader", "q"],
      "commands": [":q!"]
    },
    {
      "before": ["leader", "o"],
      "commands": [":x!"]
    },
    {
      "before": ["leader", "."],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["leader", "s", "p"],
      "commands": ["workbench.action.findInFiles"]
    },
    {
      "before": ["leader", "m"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["leader", "n"],
      "commands": ["workbench.action.splitEditorDown"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marked.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marked.next"]
    },
    {
      "before": ["leader", ";"],
      "commands": ["settings.cycle.copilot"]
    },
    {
      "before": ["leader", "s", "i"],
      "commands": ["workbench.action.showAllSymbols"]
    },
    {
      "key": "gd",
      "command": "editor.action.revealDefinition",
      "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
    },
    {
      "before": ["leader", "s", "d"],
      "commands": ["workbench.actions.view.problems"]
    },
    {
      "before": ["leader", "r"],
      "commands": ["editor.action.startFindReplaceAction"]
    },
    {
      "before": ["leader", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["leader", "h", "t"],
      "commands": ["workbench.action.selectTheme"]
    },
    {
      "before": ["u"],
      "after": [],
      "commands": [
        {
          "command": "undo"
        }
      ]
    },
    {
      "before": ["<C-r>"],
      "after": [],
      "commands": [
        {
          "command": "redo"
        }
      ]
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.iconTheme": "material-icon-theme"
}
